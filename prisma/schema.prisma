generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employee {
  id        String      @id @default(cuid())
  firstname    String   
  lastname   String
  email   String
  phonenumber   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
